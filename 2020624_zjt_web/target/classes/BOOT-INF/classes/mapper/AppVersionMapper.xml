<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pqkj.mapper.AppVersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.pqkj.entity.AppVersion">
        <id column="id" property="id" />
        <result column="app_name" property="appName" />
        <result column="version_code" property="versionCode" />
        <result column="version_name" property="versionName" />
        <result column="packages" property="packages" />
        <result column="app_type" property="appType" />
        <result column="version_path" property="versionPath" />
        <result column="down_count" property="downCount" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="remark" property="remark" />
        <result column="apk_name" property="apkName" />
        <result column="is_online" property="isOnline" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, app_name, version_code, version_name, packages, app_type, version_path, down_count, create_time, update_time, remark, apk_name, is_online
    </sql>

</mapper>
